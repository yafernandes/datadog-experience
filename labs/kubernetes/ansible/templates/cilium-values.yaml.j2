operator:
  replicas: 1
logOptions:
  format: json
kubeProxyReplacement: true
k8sServiceHost: {{ ansible_host }}
k8sServicePort: 6443
ipam:
  operator:
    clusterPoolIPv4PodCIDRList:
    - 192.168.0.0/16
prometheus:
  enabled: true
nodePort:
  enabled: true
ingressController:
  enabled: true
  default: true
  loadbalancerMode: shared
  enforceHttps: false
  service:
    type: NodePort
    insecureNodePort: 30080
    secureNodePort: 30443
gatewayAPI:
  enabled: true